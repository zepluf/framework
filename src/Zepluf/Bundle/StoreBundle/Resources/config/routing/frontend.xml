<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="storebundle.contact_us" pattern="/contact-us">
        <default key="_controller">StoreBundle:Zencart:static</default>
        <default key="parameter_main_page">contact_us</default>
    </route>

    <route id="storebundle.page_not_found" pattern="/page-not-found">
        <default key="_controller">StoreBundle:Zencart:static</default>
        <default key="parameter_main_page">page_not_found</default>
    </route>

</routes>