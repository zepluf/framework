<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Person" table="person">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="currentLastName" type="string" column="current_last_name" length="255" nullable="true"/>
    <field name="currentFirstName" type="string" column="current_first_name" length="255" nullable="true"/>
    <field name="currentMiddleName" type="string" column="current_middle_name" length="255" nullable="true"/>
    <field name="currentPersonalTitle" type="string" column="current_personal_title" length="255" nullable="true"/>
    <field name="currentSuffix" type="string" column="current_suffix" length="255" nullable="true"/>
    <field name="currentNickname" type="string" column="current_nickname" length="255" nullable="true"/>
    <field name="gender" type="string" column="gender" length="45" nullable="true"/>
    <field name="comment" type="string" column="comment" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
